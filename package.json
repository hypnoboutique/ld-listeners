{
  "name": "ld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean:ios:cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn",
    "clean:ios:derived-data": "rm -rf ios/build && rm -rf ~/Library/Developer/Xcode/DerivedData",
    "clean:ios": "yarn clean:ios:cache && yarn clean:ios:derived-data",
    "pod:install": "cd ios && pod install",
    "pod:update": "cd ios && pod update",
    "pod": "yarn && yarn pod:install"
  },
  "dependencies": {
    "launchdarkly-react-native-client-sdk": "6.0.0",
    "react": "17.0.2",
    "react-native": "0.68.0",
    "react-native-config": "^0.12.0",
    "react-native-dotenv": "^3.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@testing-library/react-native": "^7.2.0",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.66.4",
    "@types/react-native-dotenv": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-eslint": "10.0.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-airbnb-typescript": "^7.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-module-resolver": "^0.16.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-react-native": "^3.11.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.6.3",
